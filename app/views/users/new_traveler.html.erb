<%= form_for(:user, url: "/signup/trips/#{@trip.id}") do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %>
  <%= f.label :last_name %>
  <%= f.text_field :last_name %>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <%= f.label :image %>
  <%= f.text_field :image %>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <%= f.submit "Sign up"%>
<% end %>
